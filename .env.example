WAQI_TOKEN=your_token_here
WAQI_STATION=A377965

# Hiddify proxy mode (follow-system | force | off)
# follow-system — по умолчанию: не трогаем системный прокси и используем его в приложении
# force — принудительно используем HIDDIFY_PROXY только в приложении (системный не меняем, если не включить флаг ниже)
# off — принудительно отключаем прокси в приложении (direct)
HIDDIFY_MODE=follow-system

# Адрес прокси Hiddify (используется только в режиме force). Поддерживаются http://, https://, socks5://
HIDDIFY_PROXY=http://127.0.0.1:7890

# Исключения (через ;) — только для режима force
HIDDIFY_PROXY_BYPASS=localhost;127.0.0.1;<local>

# Менять системный прокси Windows (влияет на все приложения) — используется только при HIDDIFY_MODE=force
HIDDIFY_SET_SYSTEM_PROXY=false
