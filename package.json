{
  "name": "aqi-widget",
  "version": "1.0.0",
  "description": "AQI Widget Electron App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build": "electron-builder"
  },
  "build": {
  "appId": "com.yourapp.aqi",
  "win": {
    "target": "nsis"
  }
},

    "extraMetadata": {
      "main": "main.js"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "runAfterFinish": true,
      "createDesktopShortcut": false,
      "createStartMenuShortcut": false
    }
,
  "devDependencies": {
    "electron": "^29.0.0",
    "electron-builder": "^24.0.0"
  },
  "dependencies": {
    "auto-launch": "^5.0.6"
  }

}